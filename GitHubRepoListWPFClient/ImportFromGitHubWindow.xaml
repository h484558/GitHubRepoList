<Window x:Class="GitHubRepoListWPFClient.ImportFromGitHubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GitHubRepoListWPFClient"
        mc:Ignorable="d"
        Title="Import From GitHub" Height="120" Width="315" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" Icon="Git-Icon-Black.ico">
    <Grid FocusManager.FocusedElement="{Binding ElementName=gitHubUsernameTextBox}">
        <Label x:Name="label" Content="http://github.com/" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="gitHubUsernameTextBox" HorizontalAlignment="Left" Height="23" Margin="127,14,0,0" TextWrapping="Wrap" Text="google" VerticalAlignment="Top" Width="155"/>
        <Button x:Name="importFromGitHubButton" Content="Import" HorizontalAlignment="Left" Margin="127,42,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="importFromGitHubButton_Click"/>
        <Button x:Name="button1" Content="Cancel" HorizontalAlignment="Left" Margin="207,42,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>

    </Grid>
</Window>
